{
  "name": "insyht/larvelous-base-blocks",
  "type": "library",
  "description": "Basic blocks for the Larvelous CMS.",
  "keywords": [
    "cms",
    "larvelous",
    "blocks"
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Insyht\\LarvelousBaseBlocks\\Providers\\BaseBlocksServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Insyht\\LarvelousBaseBlocks\\": "src"
    }
  },
  "require": {
    "php": "^8.2"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.0"
  }
}
